<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="400" fill="#f0f4f8" />
  
  <!-- Decorative elements -->
  <circle cx="50" cy="50" r="30" fill="#FFD166" opacity="0.7" />
  <circle cx="350" cy="70" r="25" fill="#EF476F" opacity="0.6" />
  <circle cx="320" cy="320" r="35" fill="#06D6A0" opacity="0.5" />
  <circle cx="80" cy="330" r="20" fill="#118AB2" opacity="0.7" />
  
  <!-- Kid silhouette -->
  <g transform="translate(200, 200) scale(0.8)">
    <!-- Body -->
    <ellipse cx="0" cy="10" rx="50" ry="90" fill="#118AB2" opacity="0.8" />
    
    <!-- Head -->
    <circle cx="0" cy="-90" r="45" fill="#118AB2" opacity="0.8" />
    
    <!-- Arms -->
    <ellipse cx="-60" cy="-20" rx="25" ry="60" transform="rotate(-30 -60 -20)" fill="#118AB2" opacity="0.8" />
    <ellipse cx="60" cy="-20" rx="25" ry="60" transform="rotate(30 60 -20)" fill="#118AB2" opacity="0.8" />
    
    <!-- Legs -->
    <ellipse cx="-25" cy="100" rx="20" ry="65" transform="rotate(15 -25 100)" fill="#118AB2" opacity="0.8" />
    <ellipse cx="25" cy="100" rx="20" ry="65" transform="rotate(-15 25 100)" fill="#118AB2" opacity="0.8" />
  </g>
  
  <!-- Text -->
  <text x="200" y="200" font-family="Arial, sans-serif" font-size="24" font-weight="bold" text-anchor="middle" fill="#073B4C">My Cutipie</text>
  <text x="200" y="230" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#073B4C">Kids Factory Outlet</text>
  
  <!-- Playful elements -->
  <path d="M60,150 L80,120 L100,150 Z" fill="#EF476F" />
  <path d="M300,130 L320,100 L340,130 Z" fill="#06D6A0" />
  <path d="M120,300 L140,270 L160,300 Z" fill="#FFD166" />
  <path d="M240,280 L260,250 L280,280 Z" fill="#118AB2" />
  
  <!-- Stars -->
  <path d="M50,200 L55,210 L65,212 L57,220 L59,230 L50,225 L41,230 L43,220 L35,212 L45,210 Z" fill="#FFD166" />
  <path d="M330,180 L335,190 L345,192 L337,200 L339,210 L330,205 L321,210 L323,200 L315,192 L325,190 Z" fill="#EF476F" />
</svg>
